

<table class="table">
    <thead>
        <tr>
            <th scope="col">Students</th>
        </tr>
    </thead>
    <tbody>

        @foreach (var item in ViewBag.data)
        {
             <tr>
                    <td>@item.UserName</td>
                    <td>
                        <a class="btn btn-primary" asp-route-data="@item.Id" asp-route-id="@ViewBag.id" asp-action="AddThisClass">Add This Class</a>
                    </td>

                </tr>


        }
    </tbody>
</table>